    {
        "classInfo": "(BC).CIC:P",
        "objectId": 231,
        "description": "前後のテキストの状況を示すヒントになるかも",
        "originalText": "context.event_xpbl_enteringFort_movedUp",
        "ignore": true,
        "translatedText": ""
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 235,
        "description": "n/a",
        "originalText": "Area is clear, sir. No signs of life. We haven't noticed any latent security systems either.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(BC).CIC:P",
        "objectId": 238,
        "description": "前後のテキストの状況を示すヒントになるかも",
        "originalText": "context.event_xpbl_enteringFort_movedUp",
