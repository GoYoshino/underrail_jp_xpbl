    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 136,
        "description": "n/a",
        "originalText": "::She pauses.:: Oh, alright. I'm on a break anyway. ::She coughs.:: I'm Olivia. What do you need?",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(SBFV).SFVA:P",
        "objectId": 140,
        "description": "前後のテキストの状況を示すヒントになるかも",
