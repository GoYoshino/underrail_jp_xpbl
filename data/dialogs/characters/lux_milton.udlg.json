    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 423,
        "description": "n/a",
        "originalText": "::Fight!::",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 429,
        "description": "n/a",
        "originalText": "Is this the key?",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 433,
        "description": "n/a",
        "originalText": "Could use some money. It's a deal.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 437,
        "description": "n/a",
        "originalText": "I'm not going back there. Go get it yourself.",
        "translatedText": "",
