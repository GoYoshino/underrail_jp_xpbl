    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 406,
        "description": "n/a",
        "originalText": "Here you go. It was nice doing business with ya.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 411,
        "description": "n/a",
        "originalText": "Okay, okay, here's the key. My fifty charons, please.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 415,
        "description": "n/a",
        "originalText": "Sorry, buddy, no deal. I'll be leaving now.",
        "translatedText": "",
