    {
        "className": "Q",
        "objectId": 244,
        "description": "n/a",
        "originalText": "Let's talk...",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 254,
        "description": "n/a",
        "originalText": "Git outta me face.",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 259,
        "description": "n/a",
        "originalText": "Git outta me face 'fore I hurt ye.",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 264,
        "description": "n/a",
        "originalText": "Ye'll get plen'y a chances to prove yerself, banga. Plen'y.",
        "translatedText": ""
    },
    {
        "className": "LR",
        "objectId": 288,
