    },
    {
        "className": "Q",
        "objectId": 128,
        "description": "n/a",
        "originalText": "Yer one of us, banga.",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 134,
        "description": "n/a",
        "originalText": "Yer no dryfoot no more, banga. ",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 140,
        "description": "n/a",
        "originalText": "I ain' talkin' to no stinkin' Can. Ye prove yer one of us... den maybe.",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 148,
        "description": "n/a",
        "originalText": "Prove yer worth, den speak to me, dryfoot.",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 158,
        "description": "n/a",
        "originalText": "Back to d' ship!",
        "translatedText": ""
    },
    {
        "className": "Q",
        "objectId": 163,
        "description": "n/a",
        "originalText": "We're gettin' outta 'ere, den it's every pirate fer 'imself. Move yer bones!",
        "translatedText": ""
    },
