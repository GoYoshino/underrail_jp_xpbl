    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 100,
        "description": "n/a",
        "originalText": "I'd like to buy that mechanics workbench.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 105,
        "description": "n/a",
        "originalText": "You seem pretty relaxed considering what is happening right now.",
        "translatedText": "",
        "ignore": false
