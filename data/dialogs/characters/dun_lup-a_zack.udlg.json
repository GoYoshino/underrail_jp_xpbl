[
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 54,
        "description": "n/a",
        "originalText": "Are they still here?",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 94,
        "description": "n/a",
        "originalText": "We gotta figure out what to do next.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 99,
        "description": "n/a",
        "originalText": "I thought we were doomed.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 104,
        "description": "n/a",
        "originalText": "The earthquake brought so much trouble for us.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 109,
        "description": "n/a",
        "originalText": "At least we are not dead.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 118,
        "description": "n/a",
        "originalText": "I've dealt with all the bandits. You are safe now.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 121,
        "description": "n/a",
        "originalText": "Not yet.",
        "translatedText": "",
        "ignore": false
    },
