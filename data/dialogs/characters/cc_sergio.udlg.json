    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 418,
        "description": "n/a",
        "originalText": "Do people get hurt doing this?",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 422,
        "description": "n/a",
        "originalText": "I want some more.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 427,
        "description": "n/a",
        "originalText": "Meow! Of course it is, cat: It's fantastic, vivid--",
