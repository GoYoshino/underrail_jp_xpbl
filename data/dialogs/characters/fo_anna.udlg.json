    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 112,
        "description": "n/a",
        "originalText": "Goodbye.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 119,
        "description": "n/a",
        "originalText": "What about him?",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 131,
        "description": "n/a",
        "originalText": "I'm sorry, but he isn't here right now. Come back some other time, please.",
        "translatedText": "",
        "ignore": false
    },
