    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 773,
        "description": "n/a",
        "originalText": "$(context.player.Name)...",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 779,
        "description": "n/a",
        "originalText": "Father.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 784,
        "description": "n/a",
        "originalText": "Father, I do not understand anything anymore. I am malfunctioning, Father.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 792,
        "description": "n/a",
        "originalText": "I'll fix you, IRIS.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 796,
        "description": "n/a",
        "originalText": "I'm afraid so.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 800,
        "description": "n/a",
        "originalText": "I am sure you will. Thank you. Let us continue with the testing.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 806,
        "description": "n/a",
        "originalText": "Whatever happens to me, Father, know that I am thankful for everything. Let us continue with the testing.",
        "translatedText": "",
        "ignore": false
