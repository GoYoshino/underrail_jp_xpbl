    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 56,
        "description": "n/a",
        "originalText": "I spoke to Harold today at the bar. He sure had a couple of good things to say about you, friend.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 62,
        "description": "n/a",
        "originalText": "::He nods.:: Friend.",
        "translatedText": "::彼は頷く。::やあ。",
        "ignore": false
    },
    {
        "classInfo": "(Q).SE:LT:0:Value",
        "objectId": 68,
        "description": "n/a",
        "originalText": "::A lean, athletic man greets you with a friendly, resonant voice.::\r\n\r\nHello, friend. I don't believe we've met. My name is Bisson.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(SBFV).SFVA:P",
        "objectId": 92,
        "description": "前後のテキストの状況を示すヒントになるかも",
