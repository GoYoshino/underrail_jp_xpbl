    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 304,
        "description": "n/a",
        "originalText": "Any more teachers you know of?",
        "translatedText": "他に教えられそうな人に心当たりはないか？",
        "ignore": false
    },
    {
        "classInfo": "(BC).CIC:P",
        "objectId": 305,
        "description": "前後のテキストの状況を示すヒントになるかも",
