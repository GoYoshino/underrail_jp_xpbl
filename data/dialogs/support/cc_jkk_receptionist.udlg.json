        "ignore": true
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 218,
        "description": "n/a",
        "originalText": "Thank you for your time. Goodbye.",
        "translatedText": "",
        "ignore": false
    },
    {
        "classInfo": "(A3).SE:LT:0:Value",
        "objectId": 222,
        "description": "n/a",
        "originalText": "Well, I didn't even want this stupid job. So long.",
        "translatedText": "",
        "ignore": false
    },
    {
