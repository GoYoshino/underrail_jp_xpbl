    {
        "classInfo": ".Q.SE:LT:0:Value",
        "objectId": 66,
        "description": "n/a",
        "originalText": "I don't know why you paid my debt off, but thank you. Thank you! You're the boss! Those guys surely weren't messing around.",
        "translatedText": ""
    },
    {
        "classInfo": ".Q.SE:LT:0:Value",
        "objectId": 73,
        "description": "n/a",
        "originalText": "Thank you for saving me, I thought I was a goner for sure.",
