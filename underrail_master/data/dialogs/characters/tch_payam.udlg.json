        "objectId": 433,
        "description": "n/a",
        "originalText": "Principal Investigator Georgis.",
        "translatedText": ""
    },
    {
        "classInfo": ".A3.SE:LT:0:Value",
        "objectId": 438,
        "description": "n/a",
        "originalText": "Principal Investigator Schteff.",
        "translatedText": ""
    },
